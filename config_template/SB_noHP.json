{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui_download_url": "",
      "external_ui_download_detour": "",
      "external_ui": "ui",
      "secret": "",
      "default_mode": "Rule"
    },
  "cache_file": {
    "enabled": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "udp://1.0.0.1"
      },
      {
        "tag": "dns_direct",
        "address": "h3://223.6.6.6/dns-query",
        "detour": "DIRECT"
      },
      {
        "tag": "dns_fakeip",
        "address": "fakeip"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "rule_set": "geosite-ads",
        "server": "block"
      },
      {
        "outbound": "any",
        "server": "dns_direct",
        "disable_cache": true
      },
      {
        "domain_suffix": ".googleapis.cn",
        "server": "dns_proxy"
      },
      {
        "clash_mode": "Global",
        "server": "dns_fakeip",
        "rewrite_ttl": 1
      },
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "rule_set": "geosite-cn",
        "server": "dns_direct"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_fakeip",
        "rewrite_ttl": 1
      }
    ],
    "final": "dns_proxy",
    "reverse_mapping": true,
    "fakeip": {
      "enabled": true,
      "inet4_range": "22.22.0.0/16",
      "inet6_range": "fc00::/16"
    },
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",      
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "endpoint_independent_nat": false,
      "stack": "gvisor"
    },
    {
      "type": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],
  "outbounds": [
    {
      "tag":"手动选择",
      "type":"selector",
      "outbounds":[
        "自动选择",
        "{all}"
      ]
    },
    {
      "tag":"自动选择",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["网站|剩余|流量|过期|时间|期|地址"]}
      ],
      "url": "http://cp.cloudflare.com",
      "interval": "30m",
      "tolerance": 15
    },
    {
      "type": "direct",
      "tag": "DIRECT"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "REJECT"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "手动选择",
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "outbound": "DIRECT"
      },
      {
        "rule_set": "geosite-ads",
        "outbound": "REJECT"
      },
      {
        "clash_mode": "Direct",
        "outbound": "DIRECT"
      },
      {
        "clash_mode": "Global",
        "outbound": "手动选择"
      },
      {
        "domain_suffix": [
          ".googleapis.cn",
          ".yjys.me",
          ".freeok.me",
          ".duboku.tv"
        ],
        "outbound": "手动选择"
      },
      {
        "domain_keyword": [
          "vip.ffzy",
          "vip.lz-cdn",
          "vip.kuaikan",
          "v.cdnlz",
          "bfbfvip.com",
          "tscdn.hyz"
        ],
        "outbound": "DIRECT"
      },
      {
        "rule_set": [
          "geosite-cn",
          "geoip-cn"
        ],
        "outbound": "DIRECT"
      },
      {
        "rule_set": "geosite-!cn",
        "outbound": "手动选择"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-ads",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/category-ads-all.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/cn.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-cn",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-!cn",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "自动选择"
      }
    ]
  }
}
