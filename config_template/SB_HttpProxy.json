{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui_download_url": "",
      "external_ui_download_detour": "",
      "external_ui": "ui",
      "secret": "",
      "default_mode": "Rule"
    },
  "cache_file": {
    "enabled": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_proxy",
        "address": "udp://1.0.0.1",
        "detour": "自动选择"
      },
      {
        "tag": "dns_direct",
        "address": "h3://223.6.6.6/dns-query",
        "detour": "DIRECT"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "rule_set": "geosite-ads",
        "server": "block"
      },
      {
        "outbound": "any",
        "server": "dns_direct",
        "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "Global",
        "server": "dns_proxy"
      },
      {
        "rule_set": "geosite-cn",
        "server": "dns_direct"
      },
      {
        "rule_set": "geosite-!cn",
        "server": "dns_proxy"
      }
    ],
    "final": "dns_proxy",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "endpoint_independent_nat": false,
      "stack": "gvisor",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "type": "mixed",
      "listen": "0.0.0.0",
      "listen_port": 2080,
      "sniff": true,
      "users": []
    }
  ],
  "outbounds": [
    {
      "tag":"手动选择",
      "type":"selector",
      "outbounds":[
        "自动选择",
        "{all}"
      ]
    },
    {
      "tag":"ChatGPT",
      "type":"selector",
      "outbounds":[
        "手动选择",
        "{all}"
      ]
    },
    {
      "tag":"油管",
      "type":"selector",
      "outbounds":[
        "手动选择",
        "{all}"
      ]
    },
    {
      "tag":"电报",
      "type":"selector",
      "outbounds":[
        "手动选择",
        "{all}"
      ]
    },
    {
      "tag":"自动选择",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["网站|剩余|流量|过期|时间|期|地址"]}
      ],
      "url": "http://cp.cloudflare.com",
      "interval": "30m",
      "tolerance": 15
    },
    {
      "type": "direct",
      "tag": "DIRECT"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "REJECT"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "手动选择",
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "network": "udp",
        "port": 443,
        "outbound": "REJECT"
      },
      {
        "ip_is_private": true,
        "outbound": "DIRECT"
      },
      {
        "rule_set": "geosite-ads",
        "outbound": "REJECT"
      },
      {
        "clash_mode": "Direct",
        "outbound": "DIRECT"
      },
      {
        "clash_mode": "Global",
        "outbound": "手动选择"
      },
      {
        "domain_suffix": [
          ".googleapis.cn",
          ".yjys.me",
          ".freeok.me",
          ".duboku.tv"
        ],
        "outbound": "手动选择"
      },
      {
        "domain_keyword": [
          "vip.ffzy",
          "vip.lz-cdn",
          "vip.kuaikan",
          "v.cdnlz",
          "bfbfvip.com",
          "tscdn.hyz"
        ],
        "outbound": "DIRECT"
      },
      {
        "rule_set": [
          "geosite-openai"
        ],
        "outbound": "ChatGPT"
      },
      {
        "rule_set": [
          "geosite-youtube"
        ],
        "outbound": "油管"
      },
      {
        "rule_set": [
          "geosite-telegram",
          "geoip-telegram"
        ],
        "outbound": "电报"
      },
      {
        "rule_set": [
          "geosite-cn",
          "geoip-cn"
        ],
        "outbound": "DIRECT"
      },
      {
        "rule_set": "geosite-!cn",
        "outbound": "手动选择"
      }
    ],
    "rule_set": [
      {
        "type": "remote",
        "tag": "geosite-ads",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/category-ads-all.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-openai",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/openai.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-youtube",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/youtube.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geoip-telegram",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/telegram.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-telegram",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/telegram.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geoip-cn",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/cn.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-cn",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geoip/cn.srs",
        "download_detour": "自动选择"
      },
      {
        "type": "remote",
        "tag": "geosite-!cn",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/geo/geosite/geolocation-!cn.srs",
        "download_detour": "自动选择"
      }
    ]
  }
}
